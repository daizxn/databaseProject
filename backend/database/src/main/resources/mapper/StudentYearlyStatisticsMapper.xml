    <sql id="Base_Column_List">
        dzx_student_number,dzx_student_name,dzx_class_name,dzx_department_name,dzx_academic_year,dzx_average_score,
        dzx_average_gpa,dzx_total_credits,dzx_earned_credits,dzx_total_courses,dzx_passed_courses,
